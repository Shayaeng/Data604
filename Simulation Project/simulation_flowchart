digraph {
	Start [label="Start Simulation"]
	Event [label="Process Event"]
	EnergyDemand [label="Simulate Energy Demand"]
	SolarGeneration [label="Simulate Solar Generation"]
	ExcessHandling [label="Handle Excess Energy"]
	MonthlyCost [label="Calculate Monthly Costs"]
	End [label="End Simulation"]
	Start -> Event
	Event -> EnergyDemand
	Event -> SolarGeneration
	SolarGeneration -> ExcessHandling
	EnergyDemand -> ExcessHandling
	ExcessHandling -> MonthlyCost
	MonthlyCost -> Event [constraint=false]
	Event -> End
}
